{
  "name": "vanilla",
  "displayName": "vanilla",
  "description": "Language support for Vanilla.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "vanilla",
      "aliases": ["Vanilla", "vanilla"],
      "extensions": ["van"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "vanilla",
      "scopeName": "source.vanilla",
      "path": "./syntaxes/vanilla.tmLanguage.json"
    }]
  }
}
